{
  "compilerOptions": {
    "target": "ESNext",
    "jsxImportSource": "react",
    "module": "ESNext",
    "rootDir": "./src",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "moduleResolution": "node"
  }
}
